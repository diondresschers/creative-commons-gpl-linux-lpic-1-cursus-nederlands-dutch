<!-- netwerken-image-04 -->
    <div id="netwerken-image-04" style="position: absolute; transform: translate(-50%, -50%) translate3d(4000px, -1500px, 0px) rotateX(0deg) rotateY(0deg) rotateZ(0deg) scale(1); transform-style: preserve-3d; "  class="step slide past" data-x="4000" data-y="-1500">

     <div style="width: 90%;  position: absolute; top: 20px; text-align:center;"><p style="font-size:200%;">netwerken (ethernet frame)</p><p style="font-size:100%; position:relative; top:20px;"> </p></div>

<div style="position: absolute; top: 200px; left:100px; text-align:left; font-size:100%;">
